<Grid x:Class="SSpinner.Sspinner"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:sWidgets="clr-namespace:SWidgets;assembly=SWidgets"
      mc:Ignorable="d" 
      d:DesignHeight="50" d:DesignWidth="150">
    <Grid.ColumnDefinitions>
        <ColumnDefinition/>
        <ColumnDefinition Width="50"/>
    </Grid.ColumnDefinitions>
    <Grid.RowDefinitions>
        <RowDefinition/>
        <RowDefinition/>
    </Grid.RowDefinitions>
    <Grid x:Name="valueTextBoxPlaceHolder" Grid.RowSpan="2" Background="White"/>
    <sWidgets:SButton x:Name="plusButton" Content="+" Grid.Column="1"/>
    <sWidgets:SButton x:Name="minusButton" Content="-" Grid.Row="1" Grid.Column="1"/>
</Grid>
